- linux 用主机代理
export http_proxy="socks5://192.168.137.1:7890"
export https_proxy="socks5://192.168.137.1:7890"
-- 其它形式
export http_proxy="http://localhost:port"
export https_proxy="http://localhost:port"
--
export http_proxy="socks5://192.168.13.53:7890"
export https_proxy="socks5://192.168.13.53:7890"
-- 取消
unset http_proxy;unset https_proxy
